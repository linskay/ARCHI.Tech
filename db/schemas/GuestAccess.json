{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "guestAccessId": {
      "type": "string",
      "format": "uuid"
    },
    "user": {
      "$ref": "#/definitions/User"
    },
    "accessStartTime": {
      "type": "string",
      "format": "date-time"
    },
    "accessEndTime": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "enum": ["ACTIVE", "EXPIRED", "REVOKED"]
    }
  },
  "required": ["user", "accessStartTime", "accessEndTime", "status"],
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string",
          "format": "uuid"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        }
      }
    }
  }
}
